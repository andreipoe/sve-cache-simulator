  "version": "0.2.0",
  "configurations": [
    {
      "name": "(gdb) Launch make-build",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/scs",
      "args": [
        "sve-memtrace.bude-sve-arm19.2.15792.log"
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    },
    {
      "name": "(gdb) Launch meson-build",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build-debug-gnu/scs",
      "args": [
        "../sve-memtrace.bude-sve-arm19.2.15792.log"
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}/build-debug-gnu",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    },
    {
      "name": "(gdb) Launch meson-build test",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build-debug-gnu/scs-test",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}/build-debug-gnu",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}
